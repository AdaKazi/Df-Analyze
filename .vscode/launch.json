  "version": "0.2.0",
  "configurations": [
    {
      "name": "Python: Current File",
      "type": "python",
      "request": "launch",
      "program": "${file}",
      "console": "integratedTerminal"
    },
    {
      "name": "Regression IntegrationTest",
      "type": "python",
      "request": "launch",
      "program": "df-analyze.py",
      "args": [
        "--df=data/sklearn_regression/data.json",
        "--target=target",
        "--mode=regress",
        "--regressors", "svm", "linear",
        "--drop-nan=rows",
        "--feat-clean=constant",
        "--feat-select", "pca", "pearson",
        "--n-feat=10",
        "--htune",
        "--test-val=kfold",
        "--test-val-size=5",
        "--outdir=test_results"
      ],
      "console": "integratedTerminal"
    },
    {
      "name": "Regression IntegrationTest (MLP)",
      "type": "python",
      "request": "launch",
      "program": "df-analyze.py",
      "args": [
        "--df=data/sklearn_regression/data.json",
        "--target=target",
        "--mode=regress",
        "--regressors", "mlp",
        "--drop-nan=rows",
        "--feat-clean=constant",
        "--feat-select", "pca", "pearson",
        "--n-feat=10",
        "--htune",
        "--htune-trials=5",
        "--test-val=kfold",
        "--test-val-size=5",
        "--outdir=test_results"
      ],
      "console": "integratedTerminal"
    }
  ]
}